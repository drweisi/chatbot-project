{
  "builds": [
    {
      "src": "api/app.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/static/$1"
    },
    {
      "src": "/api/chat",
      "dest": "/api/app.py"
    },
    {
      "src": "/api/history",
      "dest": "/api/app.py"
    },
    {
      "src": "/",
      "dest": "/api/app.py"
    }
  ]
}
